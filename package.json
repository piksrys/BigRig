  "license": "GPL-3.0-or-later",
  "main": "index.php",
  "homepage": "https://github.com/briainod/BigRig",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/briainod/BigRig.git"
  },
  "bugs": {
    "url": "https://github.com/briainod/BigRig/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@wordpress/eslint-plugin": "^3.4.1",
    "ajv": "^6.11.0",
    "ansi-colors": "^4.1.1",
    "autoprefixer": "^9.7.4",
    "babel-jest": "^25.1.0",
    "browser-sync": "^2.26.7",
    "create-cert": "^1.0.6",
    "cross-env": "^7.0.0",
    "cssnano": "^4.1.10",
    "deepmerge": "^4.2.2",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-gulp": "^3.0.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "esm": "^3.2.25",
    "esvalidate": "^0.1.3",
    "fancy-log": "^1.3.3",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-load-plugins": "^2.0.2",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-potomo": "^1.1.0",
    "gulp-print": "^5.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.0",
    "gulp-sort": "^2.0.0",
    "gulp-string-replace": "^1.1.2",
    "gulp-stylelint": "^13.0.0",
    "gulp-tabify": "0.0.2",
    "gulp-uglify": "^3.0.2",
    "gulp-wp-pot": "^2.3.6",
    "gulp-zip": "^5.0.1",
    "import-fresh": "^3.2.1",
    "jest": "^25.1.0",
    "jest-expect-message": "^1.0.2",
    "lodash.clonedeep": "^4.5.0",
    "map-stream": "0.0.7",
    "mississippi": "^4.0.0",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "plugin-error": "^1.0.1",
    "prettier": "^1.19.1",
    "pump": "^3.0.0",
    "rimraf": "^3.0.1",
    "vinyl": "^2.2.0"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "eslintConfig": {
    "extends": "plugin:@wordpress/eslint-plugin/recommended",
    "root": true,
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "globals": {
      "jQuery": "readonly",
      "wp": "readonly"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ]
  },
  "scripts": {
    "build": "gulp buildDev",
    "bundle": "cross-env NODE_ENV=production gulp bundleTheme",
    "dev": "gulp",
    "gulp": "gulp",
    "generateCert": "gulp generateCert",
    "rig-init": "npm install && composer install",
    "translate": "gulp translate",
    "test:gulp:dev": "jest gulp/tests --testPathIgnorePatterns gulp/tests/prod-build",
    "test:gulp:prod:setup": "NODE_ENV=production node -r esm gulp/tests/prod-build/prod-build.setup.js",
    "test:gulp:prod:teardown": "NODE_ENV=production node -r esm gulp/tests/prod-build/prod-build.teardown.js",
    "test:gulp:prod:run": "cross-env NODE_ENV=production jest gulp/tests/prod-build",
    "test:gulp:prod": "run-s test:gulp:prod:setup bundle test:gulp:prod:run test:gulp:prod:teardown",
    "test:gulp": "run-s test:gulp:dev test:gulp:prod",
    "lint": "eslint assets/js/src/",
    "lint:gulp": "eslint gulp/",
    "lint:gulp:fix": "eslint --fix gulp/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.12.1",
    "bootstrap": "^4.4.1",
    "slick-carousel": "^1.8.1"
  }
}
